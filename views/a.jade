extends layout

block page
  - var menu = 'register'
block content
  .col-md-4.col-md-offset-4
    h2 Register
    div= err
    div
      form(action="/a", method="POST")
        p
          input#email.form-control(name="email", type="email", placeholder="Email: (university email)")
        p
          input.form-control(name="password", type="password", placeholder="Password: *******")
        p
          button.btn.btn-lg-primary.btn-block(type="submit") Register
